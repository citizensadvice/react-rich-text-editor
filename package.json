{
  "name": "@citizensadvice/react-rich-text-editor",
  "version": "0.1.0",
  "description": "Slate based rich text editor",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:cjs": "rm -rf cjs && babel src --out-dir cjs --source-maps",
    "build:es": "rm -rf es && BABEL_ENV=es babel src --out-dir es --source-maps",
    "prepare": "npm run build:cjs && npm run build:es",
    "install": "npm run prepare",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:js": "eslint . --ext .js --ext .jsx",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint": "npm run lint:js && npm run lint:scss"
  },
  "dependencies": {
    "react-scripts": "3.4.1",
    "@babel/cli": "^7.8.4"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.9.4",
    "@slate-editor/alignment-plugin": "^5.1.0",
    "@slate-editor/bold-plugin": "^5.1.0",
    "@slate-editor/italic-plugin": "^5.1.0",
    "@slate-editor/link-plugin": "^5.1.0",
    "@slate-editor/list-plugin": "^5.1.0",
    "@slate-editor/strikethrough-plugin": "^5.1.0",
    "@slate-editor/underline-plugin": "^5.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "classnames": "^2.2.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gh-pages": "^2.2.0",
    "is-hotkey": "^0.1.4",
    "lodash-es": "^4.17.15",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.3",
    "slate": "^0.47.8",
    "slate-editor": "^5.1.1",
    "slate-plain-serializer": "^0.7.11",
    "slate-react": "^0.22.4",
    "stylelint": "^13.3.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.17.0"
  },
  "peerDependencies": {
    "prop-types": ">= 15",
    "react": ">= 16.8"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": false
}
